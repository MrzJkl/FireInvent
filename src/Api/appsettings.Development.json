{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=fireinvent_db;Username=fireinvent;Password=fireinvent"
  },
  "Authentication": {
    "Authority": "http://localhost:8080/realms/fireinvent/",
    "OidcDiscoveryUrlForSwagger": "http://localhost:8080/realms/fireinvent/.well-known/openid-configuration"

  }
}
